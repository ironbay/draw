{
	"version": "2.1.5",
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"lib": ["dom","es5","scripthost", "es2015.promise", "es2015.collection", "es2017.object"],
		"jsx": "react",
		"noUnusedLocals": false,
		"strictNullChecks": false,
		"removeComments": false
	}
}
